{
    "name": "solatyolo/lightcache",
    "type": "library",
    "description": "a light weight cache lib",
    "keywords": [ "psr-16", "cache", "light-cache","redis","memory"],
    "license": "MIT",
    "homepage": "https://github.com/SolaTyolo/lightcache",
    "autoload": {
        "psr-4": {
            "SolaTyolo\\Lightcache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SolaTyolo\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "~7.3 || ^8.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "predis/predis": "^1.1",
        "phpunit/phpunit": "^8.0 || ^9.0"
    }
}
